{
  "manifest_version": 3,
  "name": "MoeKoe Music Total Play time",
  "version": "1.0.0",
  "description": "MoeKoe Music 累计听歌时长计算",
  "permissions": ["storage"], 
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "累计听歌时长请搭配时长显示插件使用"
  },
  "icons": {
    "256": "icons.jpg"
  }
}